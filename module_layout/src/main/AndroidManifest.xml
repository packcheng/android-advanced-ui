<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.packcheng.layout">

    <application
        android:name=".debug.LayoutApplication"
        android:allowBackup="true"
        android:icon="@mipmap/layout_ic_launcher"
        android:label="@string/layout_app_name"
        android:roundIcon="@mipmap/layout_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".LayoutActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
    </application>

</manifest>